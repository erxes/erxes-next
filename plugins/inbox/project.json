{
  "name": "inbox",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "plugins/inbox/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "target": "web",
        "outputPath": "dist/plugins/inbox",
        "main": "plugins/inbox/src/main.ts",
        "tsConfig": "plugins/inbox/tsconfig.app.json",
        "rspackConfig": "plugins/inbox/rspack.config.ts",
        "assets": [
          "plugins/inbox/src/favicon.ico",
          "plugins/inbox/src/assets"
        ]
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "optimization": true,
          "sourceMap": false,
          "rspackConfig": "plugins/inbox/rspack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/rspack:module-federation-dev-server",
      "options": {
        "buildTarget": "inbox:build:development",
        "port": 3002
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "inbox:build:production"
        }
      }
    },
    "serve-static": {
      "executor": "@nx/rspack:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "inbox:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "inbox:serve:development"
        },
        "production": {
          "serveTarget": "inbox:serve:production"
        }
      }
    }
  }
}
