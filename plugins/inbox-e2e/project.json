{
  "name": "inbox-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "plugins/inbox-e2e/src",
  "tags": [],
  "implicitDependencies": [
    "inbox"
  ],
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "plugins/inbox-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "inbox:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "inbox:serve:production"
        },
        "ci": {
          "devServerTarget": "inbox:serve-static"
        }
      }
    }
  }
}
