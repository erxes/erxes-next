{
  "name": "tasks-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/plugins/tasks-ui/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "web",
        "outputPath": "dist/apps/plugins/tasks-ui",
        "main": "apps/plugins/tasks-ui/src/main.ts",
        "tsConfig": "apps/plugins/tasks-ui/tsconfig.app.json",
        "rspackConfig": "apps/plugins/tasks-ui/rspack.config.ts",
        "assets": [
          "apps/plugins/tasks-ui/src/favicon.ico",
          "apps/plugins/tasks-ui/src/assets"
        ]
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "optimization": true,
          "sourceMap": false,
          "rspackConfig": "apps/plugins/tasks-ui/rspack.config.prod.ts"
        }
      }
    },
    "serve": {
      "executor": "@nx/rspack:module-federation-dev-server",
      "options": {
        "buildTarget": "tasks-ui:build:development",
        "port": 3002
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "tasks-ui:build:production"
        }
      }
    },
    "serve-static": {
      "executor": "@nx/rspack:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "tasks-ui:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "tasks-ui:serve:development"
        },
        "production": {
          "serveTarget": "tasks-ui:serve:production"
        }
      }
    }
  }
}
