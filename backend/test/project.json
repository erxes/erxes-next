{
  "name": "test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "backend/test/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",

      "options": {
        "cwd": "backend/test",
        "command": "pnpm build"
      }
    },

    "start": {
      "executor": "nx:run-commands",
      "dependsOn": ["typecheck", "build"],
      "options": {
        "cwd": "backend/test",
        "command": "NODE_ENV=development && node dist/src/main.js"
      }
    },

    "serve": {
      "executor": "nx:run-commands",

      "options": {
        "cwd": "backend/test",
        "command": "pnpm dev"
      }
    },

    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f backend/test/Dockerfile . -t test"
    }
  }
}
